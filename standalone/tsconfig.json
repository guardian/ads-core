{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "esnext",
      "es2015",
      "dom",
      "dom.iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "paths": {
      "bootstraps/*": [
        "bootstraps/*"
      ],
      "commercial/*": [
        "projects/commercial/*"
      ],
      "common/*": [
        "projects/common/*"
      ],
      "raven": [
        "node_modules/raven-js"
      ],
      "svgs/*": [
        "./static/svg/*"
      ],
	  "lodash/*": [
        "node_modules/lodash-node/compat/*"
      ],
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNEXT"
  },
  "exclude": ["node_modules", "dist", "coverage", ".eslintrc.js"]

}
